uses "http://hl7.org/fhir/4.0/StructureDefinition/FormularyItem" alias FormularyItemR4 as source
uses "http://hl7.org/fhir/StructureDefinition/FormularyItem" alias AccountR5 as target

imports "http://hl7.org/fhir/StructureMap/*4to5

group FormularyItem(source src : FormularyItemR4, target tgt : FormularyItemR5) extends DomainResource <<type+>> {
