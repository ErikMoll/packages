/// url = 'http://hl7.org/fhir/StructureMap/BackboneType4Bto5'
/// name = 'BackboneType4Bto5'
/// title = 'FML Conversion for BackboneType: R4B to R5'

uses "http://hl7.org/fhir/4.3/StructureDefinition/BackboneType" alias BackboneTypeR4B as source
uses "http://hl7.org/fhir/StructureDefinition/BackboneType" alias BackboneTypeR5 as target

imports "http://hl7.org/fhir/StructureMap/*4Bto5"

group BackboneType(source src : BackboneTypeR4B, target tgt : BackboneTypeR5) extends DataType <<type+>> {
  src.modifierExtension -> tgt.modifierExtension;
}

