/// url = 'http://hl7.org/fhir/StructureMap/DeviceAssociation5to4'
/// name = 'DeviceAssociation5to4'
/// title = 'FML Conversion for DeviceAssociation: R5 to R4'

uses "http://hl7.org/fhir/StructureDefinition/DeviceAssociation" alias DeviceAssociationR5 as source
uses "http://hl7.org/fhir/4.0/StructureDefinition/DeviceAssociation" alias DeviceAssociationR4 as target

imports "http://hl7.org/fhir/StructureMap/*5to4"

group DeviceAssociation(source src : DeviceAssociationR5, target tgt : DeviceAssociationR4) extends DomainResource <<type+>> {
  src.identifier -> tgt.identifier;
  src.device -> tgt.device;
  src.category -> tgt.category;
  src.status -> tgt.status;
  src.statusReason -> tgt.statusReason;
  src.subject -> tgt.subject;
  src.bodyStructure -> tgt.bodyStructure;
  src.period -> tgt.period;
  src.operation as s -> tgt.operation as t then DeviceAssociationOperation(s,t);
}

group DeviceAssociationOperation(source src, target tgt) extends BackboneElement {
  src.operator -> tgt.operator;
  src.period -> tgt.period;
}

