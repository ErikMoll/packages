/// url = 'http://hl7.org/fhir/StructureMap/BackboneType5to4B'
/// name = 'BackboneType5to4B'
/// title = 'FML Conversion for BackboneType: R5 to R4B'

uses "http://hl7.org/fhir/StructureDefinition/BackboneType" alias BackboneTypeR5 as source
uses "http://hl7.org/fhir/4.3/StructureDefinition/BackboneType" alias BackboneTypeR4B as target

imports "http://hl7.org/fhir/StructureMap/*5to4B"

group BackboneType(source src : BackboneTypeR5, target tgt : BackboneTypeR4B) extends DataType <<type+>> {
  src.modifierExtension -> tgt.modifierExtension;
}

